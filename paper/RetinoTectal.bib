@article{brennan_two_1997,
	title = {Two {Eph} receptor tyrosine kinase ligands control axon growth and may be involved in the creation of the retinotectal map in the zebrafish},
	volume = {124},
	issn = {0950-1991},
	url = {https://doi.org/10.1242/dev.124.3.655},
	doi = {10.1242/dev.124.3.655},
	abstract = {The isolation and characterisation of two zebrafish Eph receptor ligand cDNAs which we have called zfEphL3 and zfEphL4 is described. These genes are expressed in the presumptive midbrain of developing embryos from 6 somites. By 24 hours L3 is expressed throughout the midbrain including the region of the presumptive tectum whereas L4 is strongly expressed in the midbrain caudal to the presumptive tectum. At later stages of development L3 is expressed in a graded fashion throughout the tectum and L4 is maintained at its posterior margin. Growth cone collapse and pathway selection assays demonstrate that both these proteins have a collapse activity for retinal ganglion cells. When faced with a choice of substrate on which to grow, temporal axons from chick retinal ganglion cells selectively avoided membranes from Cos cells transfected with L3, whereas nasal axons did not. Both temporal and nasal axons avoided membranes from Cos cells transfected with L4. The expression patterns together with the functional data suggest that although both ligands may be able to guide retinal ganglion cells axons in vitro, they have different roles in the guidance of retinotectal projections in vivo. The expression of L3 is consistent with a role in the guidance of retinal ganglion cells to their targets on the tectum whereas that of L4 suggests a role in delineating the posterior boundary of the optic tectum.},
	number = {3},
	urldate = {2021-07-20},
	journal = {Development},
	author = {Brennan, C. and Monschau, B. and Lindberg, R. and Guthrie, B. and Drescher, U. and Bonhoeffer, F. and Holder, N.},
	month = feb,
	year = {1997},
	pages = {655--664},
	file = {Full Text PDF:/home/seb/Zotero/storage/RYP878KB/Brennan et al. - 1997 - Two Eph receptor tyrosine kinase ligands control a.pdf:application/pdf;Snapshot:/home/seb/Zotero/storage/RWUBN5YG/Two-Eph-receptor-tyrosine-kinase-ligands-control.html:text/html},
}

@article{noauthor_shared_1997,
