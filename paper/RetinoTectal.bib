@article{rodger_transient_2000,
	title = {Transient {Up}-{Regulation} of the {Rostrocaudal} {Gradient} of {Ephrin} {A2} in the {Tectum} {Coincides} with {Reestablishment} of {Orderly} {Projections} during {Optic} {Nerve} {Regeneration} in {Goldfish}},
