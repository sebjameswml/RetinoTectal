	year = {2004},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/neu.10341},
	keywords = {axon guidance, axon branching, axon arborization, axon competition, axon elimination, axon repellents, bidirectional signaling, ephrinA knockout mice, Isl2-EphA3 knockin mice, retinotectal projection, topographic mapping},
	pages = {95--113},
