	number = {1},
	urldate = {2021-06-28},
	journal = {Experimental Cell Research},
	author = {Wu, Zhonglin and Ashlin, Tim G. and Xu, Qiling and Wilkinson, David G.},
	month = aug,
	year = {2019},
	keywords = {Eph receptor, Ephrin, Border sharpening, Cell segregation, Cell signaling, Contact repulsion},
	pages = {57--65},
	file = {ScienceDirect Full Text PDF:/home/seb/Zotero/storage/NU6CKA5J/Wu et al. - 2019 - Role of forward and reverse signaling in Eph recep.pdf:application/pdf;ScienceDirect Snapshot:/home/seb/Zotero/storage/RW4XSRFU/S0014482719302319.html:text/html},
}

