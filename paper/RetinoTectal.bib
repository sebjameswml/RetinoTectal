	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/cne.23248},
	keywords = {visual system, topographic maps, neural development, pathfinding, retinal ganglion cell, time lapse},
	pages = {1409--1429},
	file = {Snapshot:/home/seb/Zotero/storage/SI6VFHD7/cne.html:text/html;Full Text PDF:/home/seb/Zotero/storage/FI85RD9H/Simpson et al. - 2013 - A quantitative analysis of branching, growth cone .pdf:application/pdf},
