	month = jan,
	year = {2005},
	pmid = {16353341},
	keywords = {axon guidance, Eph receptors, Topographic maps, computational model, ephrins},
	pages = {5--34},
	file = {Snapshot:/home/seb/Zotero/storage/BD6FDU9S/09548980500254654.html:text/html;Full Text PDF:/home/seb/Zotero/storage/YVBZEXAF/Goodhill and Xu - 2005 - The development of retinotectal maps A review of .pdf:application/pdf;Goodhill and Xu - 2005 - The development of retinotectal maps A review of .pdf:/home/seb/Zotero/storage/FMBVQR7R/Goodhill and Xu - 2005 - The development of retinotectal maps A review of .pdf:application/pdf},
}

@article{nakamoto_topographically_1996,
