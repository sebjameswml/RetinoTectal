	file = {ScienceDirect Full Text PDF:/home/seb/Zotero/storage/EIM3PV58/Rashid et al. - 2005 - Opposing Gradients of Ephrin-As and EphA7 in the S.pdf:application/pdf;ScienceDirect Snapshot:/home/seb/Zotero/storage/RHDB2QV6/S0896627305004782.html:text/html},
}

@article{cang_developmental_2013,
	title = {Developmental {Mechanisms} of {Topographic} {Map} {Formation} and {Alignment}},
	volume = {36},
	issn = {0147-006X, 1545-4126},
	url = {http://www.annualreviews.org/doi/10.1146/annurev-neuro-062012-170341},
	doi = {10.1146/annurev-neuro-062012-170341},
	abstract = {Brain connections are organized into topographic maps that are precisely aligned both within and across modalities. This alignment facilitates coherent integration of different categories of sensory inputs and allows for proper sensorimotor transformations. Topographic maps are established and aligned by multistep processes during development, including interactions of molecular guidance cues expressed in gradients; spontaneous activity-dependent axonal and dendritic remodeling; and sensory-evoked plasticity driven by experience. By focusing on the superior colliculus, a major site of topographic map alignment for different sensory modalities, this review summarizes current understanding of topographic map development in the mammalian visual system and highlights recent advances in map alignment studies. A major goal looking forward is to reveal the molecular and synaptic mechanisms underlying map alignment and to understand the physiological and behavioral consequences when these mechanisms are disrupted at various scales.},
	language = {en},
	number = {1},
	urldate = {2021-06-25},
	journal = {Annual Review of Neuroscience},
	author = {Cang, Jianhua and Feldheim, David A.},
	month = jul,
	year = {2013},
	pages = {51--77},
	file = {Cang and Feldheim - 2013 - Developmental Mechanisms of Topographic Map Format.pdf:/home/seb/Zotero/storage/YFQR8RG7/Cang and Feldheim - 2013 - Developmental Mechanisms of Topographic Map Format.pdf:application/pdf},
}

@article{hayes_normal_1988,
	title = {Normal and regenerating optic fibers in goldfish tectum: {HRP}-{EM} {Evidence} for rapid synaptogenesis and optic fiber-fiber affinity},
	volume = {274},
