/*
 * RT projection with how many projections?
 */
{
    // Global simulation parameters
    "steps" : 20000,                // Number of steps to simulate for
    "logevery": 0,
    "overwrite_logs": true,
    "logbase" : "./log/",
    "hextohex_d" : 0.02,           // Hex to hex distance, determines num hexes
    "svgpath" : "./boundaries/ellipse.svg",
    "boundaryFalloffDist" : 0.03,
    // Diffusion const
    "D" : 0.03,
    // gamma gain
    "G" : 6.0,
    // Exponent on a
    "k" : 3,
    "N" : 48,
    "alpha" : 20,
    "beta" : 3,
    "epsilon" : 650,
    // Timestep. Defaults to 0.00001 if omitted here
    "dt" : 0.0005,

    "contour_threshold" : 0.5,

    // Visualization parameters (ignored by computation-only binaries)
    "win_width_contours": 800,
    "plotevery": 500,
    "vidframes": false,    // If true, number video frame saves consecutively, rather than by simulation step number
    "rhoInit": 4, // Larger to zoom out
    //
    "plot_guide" : true,
    "plot_contours" : true,
    "plot_a_contours" : true,
    "plot_a" : false,
    "plot_c" : false,
    "plot_n" : true,

    "aNoiseGain" : 0.2,
    "aInitialOffset" : 0.2,

    // Array of parameters for the guidance molecules
    "guidance": [
        {
            "shape"  : "Linear1D", // 'x'
            "gain"   : 1.0,
            "phi"    : 0,
            "width"  : 0.1,
            "offset" : 0.0
        },
        {
            "shape"  : "Linear1D", // 'y'
            "gain"   : 1.0,
            "phi"    : -90,
            "width"  : 0.1,
            "offset" : 0.0
        }
    ] // end guidance
}
